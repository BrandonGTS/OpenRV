border-left: 0px solid black;
border-right: 0px solid black;
border-bottom: 1px solid black;
background-color: black;
padding: 0px;
max-height: 20px;
min-height: 0px;
}

QToolBar[tbstyle="play_controls"] {
text-align: center;
spacing: 0px;
margin: 0px;
qproperty-iconSize: 18px 18px;
border-top: 1px solid black;
border-left: none;
border-right: none;
border-bottom: none;
background-color: rgb(20,20,20);
padding: 0px;
max-height: 50px;
min-height: 50px;
}


QListView {
icon-size: 16px; 
}

QDialogButtonBox {
dialogbuttonbox-buttons-have-icons: 0;
}

QTabWidget::pane {
border: 1px outset rgb(25,25,25);
}

QLabel {
color: rgb(160,160,160);

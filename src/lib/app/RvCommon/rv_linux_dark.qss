}

QSlider {
min-width: 80px;
}

QSlider::groove {
background-color: rgb(50,50,50);
border: none;
border-radius: 3px;
margin: 6px;
}

QSlider::groove::horizontal {
height: 6px;
background-color: rgb(60,60,60);
}

QSlider::groove::vertical {
width: 6px;
background-color: rgb(60,60,60);
}

QSlider::handle {
border-radius: 8px;
background-color: rgb(130,130,130);
border: 1px solid rgb(35,35,35);
}

QSlider::handle::horizontal {
width: 14px;
margin: -4px -7px;
}

QSlider::handle::vertical {
height: 14px;
margin: -7px -4px;
}


QSlider[style="menu"] {
margin: 0px;
padding: 0px;
width: 10px;
}

QMenu {
color: rgb(160,160,160); 
font-size: %1pt; 
spacing: 0em;
border-radius: 2px;
border: 1px solid black;
background-color: rgb(20, 20, 20);
selection-color: rgb(160, 160, 160);
selection-background-color: rgb(40, 40, 40);
}

QMenu::item {
background: transparent;
color: rgb(160,160,160);
selection-color: rgb(200, 200, 200);
selection-background-color: rgb(40,40,40);
}

QMenu::item:disabled {
background: transparent;
color: rgb(80,80,80);

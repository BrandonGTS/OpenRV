}

QGroupBox {
border: 2px solid rgb(50,50,50);
border-radius: 2px;
margin: 1em;
}

QGroupBox::title {
color: rgb(90,90,90);
subcontrol-origin: margin;
subcontrol-position: top left;
left: 16px;
top: 8px;
